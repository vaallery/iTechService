<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>KeepAlive</key>
	<true/>
	<key>Label</key>
	<string>itech.itechservice.unicorn</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/var/www/itechservice/current/unicorn_init.sh</string>
		<string>start</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>UserName</key>
	<string>itech</string>
	<key>WorkingDirectory</key>
	<string>/usr/local/var/www/itechservice/current</string>
	<key>StandardErrorPath</key>
	<string>/usr/local/var/www/itechservice/current/log/unicorn.log</string>
</dict>
</plist>
